<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>$(AssemblyName)</Authors>
    <Company>Sendee</Company>
    <Description>A REST API helper for Sendee</Description>
    <Copyright>Copyright @ sendee</Copyright>
    <PackageProjectUrl>https://github.com/TeamSendee/sendee-dotnet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/TeamSendee/sendee-dotnet</RepositoryUrl>
    <PackageTags>sendee;api;rest;twilio;sms;text</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
